import random  # Import the 'random' module
length = 5 # Change depending on your needs.
usableCharacters = "01234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ" # Add or remove depending on your needs. 
password =  "".join(random.sample(usableCharacters,length))  # Generates password.
print(password) # Displays the generated password.

